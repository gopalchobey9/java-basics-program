import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int a,b;
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the value of first number1");
		a=sc.nextInt();
		System.out.println("enter the value of first number2");
		b=sc.nextInt();
		int temp;
		temp=a;
		a=b;
		b=temp;
		System.out.println("the 1st no. is now "+a+"\nsecond is now "+b);
		
		
	}
}
