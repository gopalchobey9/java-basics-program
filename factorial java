import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String name = sc.nextLine();
		String reverse="";
		int l=name.length();
		for (int i=l-1;i>=0;i--){
		    reverse=reverse+name.charAt(i);
		}
		if(name.equals(reverse))
		{
		    System.out.println("palindrome");
		    
		}
		else{
		    System.out.println("not");
		}
	}
}
